<Application x:Class="PageSelect.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PageSelect"
             xmlns:login="clr-namespace:PageSelect.Views.Login"
             xmlns:home="clr-namespace:PageSelect.Views.Home"
             StartupUri="MainView.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type home:HomeView}">
            <home:HomeView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type login:LoginView}">
            <login:LoginView/>
        </DataTemplate>
    </Application.Resources>
</Application>
